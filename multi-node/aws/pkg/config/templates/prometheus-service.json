{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "name": "prometheus"
    },
    "name": "prometheus"
  },
  "spec": {
    "ports": [
      {
        "port": 9090,
        "protocol": "TCP",
        "targetPort": 9090
      }
    ],
    "selector": {
      "app": "prometheus"
    },
    "type": "ClusterIP"
  }
}
